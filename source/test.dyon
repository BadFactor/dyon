fn main() {
    file := "source/test.dyon"
    source := unwrap(load_string(file: file))

    d := unwrap(load(meta: "assets/syntax.txt", file: file))

    for i {
        if d[i][2] != "start" {continue}
        name := d[i][3]
        if name != "fn" {continue}
        start := d[i][0]
        len := d[i][1]
        println(errstr(string: source, start: start, len: len, msg: ""))
    }
}
