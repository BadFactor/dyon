fn main() {
    a := \(x) = \(y) = \(z) = z + (grab y) + (grab '2 x)
    // prints `\(x: any) = \(y: any) = \(z: any) = z + (grab y) + (grab '2 x)`
    println(a)
    b := \a(1)
    // prints `\(y: any) = \(z: any) = z + (grab y) + 1`
    println(b)
    c := \b(2)
    // prints `\(z: any) = z + 2 + 1`
    println(c)
    d := \c(3)
    println(d)
}
