sum_n(i, n) = ∑ j n { i * n + j + 1 }

fn results(mut threads) -> res {
    return ok(∑ _ len(threads) {
        join(thread: pop(mut threads))?
    })
}

fn main() {
    n := 10000000

    t := 10
    threads := sift i t { go sum_n(i, n / t) }
    println(unwrap(results(mut threads)))

    // println(foo(0, n))
}
