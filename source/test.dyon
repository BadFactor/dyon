fn main() {
    f := \(x) = x + 1

    a := fmap(f, twice())
    println(\a(0))
}

fn twice() -> \(\(any) -> any, any) -> any {
    return \(f, v) = \f(\f(v))
}

fn fmap(f: \(any) -> any, g: \(\(any) -> any, any) -> any) -> \(any) -> any {
    return \(x) = {
        f := grab f
        g := grab g
        \g(f, x)
    }
}
