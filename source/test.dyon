fn main() {
    x := 2
    y := 2
    println(-x) // `-2`
    println(x^y) // `4`
    println(-x^y) // `-4`
    println(-x^y+3) // `-1`
    println((-x)^y) // `4`
    println((-x)^y+3) // `7`
    println(x * y) // `4`
    println(-x * y + 1) // `-3`
    println((-x) * (-y)) // `4`
    println(-x * (-y) + 1) // `5`
    println(-x * -y) // `4`
}
