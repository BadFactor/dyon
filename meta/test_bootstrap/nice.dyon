{
    rules: [
        {
            name: "pos",
            fields: [
                {name: "x", optional: false, type: "f64"},
                {name: "y", optional: false, type: "f64"},
                {name: "z", optional: true, type: "f64"}
            ],
            expr: "(x, y, if z == none() { 0 } else { unwrap(z) })"
        },
        {
            name: "doc",
            repeat: {as: some("pos"), rule: "pos"}
        }
    ],
    start: "doc"
}
